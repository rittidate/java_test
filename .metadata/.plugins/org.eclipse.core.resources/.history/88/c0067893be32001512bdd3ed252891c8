
public class CompareString {
	
	public static void main(String args[]){
		String s1 = "hello";
		String s2 = "hello";
		
		if (s1 == s1){
			System.out.println("Match");
		}else{
			System.out.println("No match");
		}
		
		if(s1.equals(s2)){
			System.out.println("Match");
		}else{
			System.out.println("No match");
		}
	}
}
